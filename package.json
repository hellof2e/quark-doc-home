{
  "name": "quark-doc-home",
  "version": "2.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hellof2e/quark-doc-home.git"
  },
  "scripts": {
    "dev": "npm run start",
    "start": "vite --force --open",
    "update": "npm i quark-doc-header",
    "build": "npm run update && vite build --config vite.config.build.ts",
    "release:patch": "npm run release -- --release-as patch",
    "release": "standard-version",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "main": "./lib/index.umd.js",
  "module": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.umd.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "keywords": [
    "web components",
    "quark",
    "quarkc",
    "quarkd"
  ],
  "dependencies": {
    "quark-doc-header": "^0.3.30",
    "quarkc": "^1.0.38"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^20.2.5",
    "less": "^4.1.3",
    "postcss": "^8.4.24",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.0"
  },
  "engines": {
    "node": ">=14.18.0"
  }
}
